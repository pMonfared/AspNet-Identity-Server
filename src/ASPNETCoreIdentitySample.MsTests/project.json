{
    "version": "1.0.0-*",

    "testRunner": "mstest",
    "dependencies": {
        "ASPNETCoreIdentitySample.DataLayer": "1.0.0-*",
        "ASPNETCoreIdentitySample.Entities": "1.0.0-*",
        "ASPNETCoreIdentitySample.Services": "1.0.0-*",
        "ASPNETCoreIdentitySample.ViewModels": "1.0.0-*",
        "Microsoft.EntityFrameworkCore": "1.1.0",
        "Microsoft.EntityFrameworkCore.InMemory": "1.1.0",
        "dotnet-test-mstest": "1.1.2-preview",
        "Microsoft.Extensions.Configuration": "1.1.0",
        "Microsoft.Extensions.Configuration.Binder": "1.1.0",
        "Microsoft.Extensions.Configuration.Json": "1.1.0",
        "MSTest.TestFramework": "1.0.8-rc2"
    },

    "buildOptions": {
        "copyToOutput": {
            "include": [
                "../ASPNETCoreIdentitySample/appsettings.json" // for unit tests
            ]
        }
